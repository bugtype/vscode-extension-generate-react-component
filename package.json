{
	"name": "generate-react-component",
	"displayName": "generate-react-component",
	"description": "Generating React Component",
	"publisher": "bugtype",
	"repository": {
		"type": "git",
		"url": "https://github.com/bugtype/vscode-extension-generate-react-component"
	},
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:generate-react-component.generate.react.component",
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "generate-react-component.generate.react.component",
				"title": "Generate React component (html element)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"vscode:publish": "vsce publish",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.11",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/react": "^17.0.8",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"prettier": "^2.3.0",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"fs-extra": "^10.0.0",
		"react": "^17.0.2"
	}
}
